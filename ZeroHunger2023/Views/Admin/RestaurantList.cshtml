
@{
    ViewBag.Title = "RestaurantList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>RestaurantList</h2>

<br />
<a class="btn btn-warning" href="/Admin/RestaurantAdd">ADD</a>
<br />
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Phone</th>
        <th>Address</th>
        

    </tr>

    @foreach (var data in Model)
    {
        <tr>
            <td>@data.ID</td>
            <td>@data.Name</td>
            <td>@data.Phone</td>
            <td>@data.Address</td>

            <td><a class="btn btn-secondary" href="/Admin/RestaurantEdit/@data.ID">Edit</a></td>
            <td><a class="btn btn-danger" href="/Admin/RestaurantDelete/@data.ID">Delete</a></td>
        </tr>


    }

</table>
